    "def fib(n):\n",
    "    fib1 = 1\n",
    "    fib2 = 1\n",
    "    yield fib1\n",
    "    yield fib2\n",
    "    for i in range(n-1):\n",
    "        fib1,fib2 = fib2, fib1 + fib2\n",
    "        yield fib2\n",
    "\n",
    "gen = fib(10)\n",
    "print(next(gen))\n",
    "\n",
    "\n",
    "for i in gen:\n",
    "    print(i)\n",
    "    "
